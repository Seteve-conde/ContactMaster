@model Dominio.Models.ContatoModel
@{
    ViewData["Title"] = "Criar Contato";
}

<div class="text-center">
    <h3>Realmente deseja apagar o contato(@Model.Nome)?</h3>
    <br />
    <a class="btn btn-primary" asp-action="Index" asp-controller="Contato" role="button"  >Canselar exclusão</a>
    <a class="btn btn-danger" asp-action="Apagar" asp-controller="Contato" asp-route-id="@Model.Id" role="button">Confirmar exclusão</a>
</div>


